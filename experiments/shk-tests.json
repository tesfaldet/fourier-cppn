{
    "title": "texture-cppn",
    "worker_config": {
        "memory": 8,
        "cpu": 2,
        "gpu": 1,
        "volumes": ["/mnt/projects/texture-cppn/logs:/logs", "/mnt/projects/texture-cppn/snapshots:/snapshots", "/mnt/datasets/public/texture-cppn/data:/data"]
    },
    "parameters": {
        "name": "GridSearch",
        "config": {
            "dimensions": {
                "learning_rate": [5e-4],
                "batch_size": [1],
                "dimensions": ["224,224"],
                "log_dir": ["/logs"],
                "snap_dir": ["/snapshots"],
                "data_dir": ["/data"],
                "cppn_coordinate_limit": ["3**0.5"],
                "cppn_num_layers": [8],
                "cppn_num_neurons": [24],
                "cppn_activation": ["atan_concat"],
                "style_layers": ["conv1_1/Relu,pool1,pool2,pool3,pool4"],
                "cppn_fourier_dimensions": ["10,10"]
            }
        }
    }
}